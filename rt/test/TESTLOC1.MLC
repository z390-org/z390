         TITLE 'TESTLOC1 - TEST MULIPLE LOCTRS'

TESTLOC1 SUBENTRY
INS      LOCTR
         WTO   'TESTLOC1 STARTED'
DATA     LOCTR
X        DS    X
INS      LOCTR
         LA    1,X
         CL    1,=A(X)
         BNE   1
         WTO   'TESTLOC1 ENDED OK'
         SUBEXIT
         END