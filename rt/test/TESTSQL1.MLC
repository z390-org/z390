         TITLE 'TESTSQL1 - TEST EXEC SQL INCLUDE SUPPORT'

TESTSQL1 SUBENTRY
         WTO   'TESTSQL1 - TEST EXEC SQL INCLUDE TESTSQL1'
         EXEC SQL INCLUDE TESTSQL1
         WTO   'TESTSQL1 ENDED OK'
         SUBEXIT
         END